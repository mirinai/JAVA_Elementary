package ex2_switch;

public class Ex1_switch {
	public static void main(String[] args) {
		//if문과 비슷, switch는 소괄호 안에 비교값이 들어간다.
		
		/*
		 * switch(비교값){
		 * case 조건값1:
		 * 		비교값과 조건값1이 같을 때 할 명령
		 * 		break;
		 * case 조건값2:
		 * 		비교값과 조건값2이 같을 때 할 명령
		 * 		break;
		 * case 조건값3:
		 * 		비교값과 조건값3이 같을 때 할 명령
		 * 		break;
		 * case 조건값4:
		 * 		비교값과 조건값4이 같을 때 할 명령
		 * 		break;
		 * default:
		 * 		비교값과 같은 조건값이 없을 때 할 명령
		 * 		break;
		 * 
		 * }
		 */
		int n=1;
		
		switch(n) {
		case 1:
			System.out.println("1번 고름");
			break;
		case 2:
			System.out.println("2번 고름");
			break;
		case 3:
			System.out.println("3번 고름");
			break;
		default:
			System.out.println("같은 값 없음");
			break;
		}
	}

}
